{
  "name": "@octopost/module-manager",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext=ts,tsx",
    "lint:fix": "pnpm lint --fix",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/react": "18.2.52",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "eslint": "8.55.0",
    "express": "4.18.2",
    "prettier": "3.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@devhatt/global": "5.0.0",
    "eventemitter3": "5.0.1",
    "h3": "1.10.1"
  }
}
