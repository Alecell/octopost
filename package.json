{
  "name": "octopost",
  "version": "0.1.0",
  "engines": {
    "node": "18.18.2"
  },
  "packageManager": "yarn@1.22.19",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "eject": "vite eject",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "vitest",
    "test:ct": "yarn test-ct",
    "test:e2e": "playwright test",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && electron-builder --windows --config electron-builder.json",
    "electron:dist": "yarn build && tsc -p electron && electron-builder --mac --dir",
    "prepare": "husky install",
    "stylelint": "stylelint \"**/*.scss\"",
    "ladle": "ladle serve",
    "test-ct": "playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-vitest": "^0.3.10",
    "framer-motion": "^10.16.1",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "jsdom": "^22.1.0",
    "nanoid": "^5.0.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.1",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@ladle/react": "^2.17.2",
    "@playwright/experimental-ct-react": "^1.37.0",
    "@playwright/test": "^1.37.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cz-format-extension": "^1.5.1",
    "electron": "^24.1.2",
    "electron-builder": "^24.0.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.12",
    "eslint-define-config": "^2.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-native": "^0.2.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "wait-on": "^7.0.1",
    "zustand": "^4.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
